<keymap version="1" name="Default for XWin copy" parent="Default for XWin">
  <action id="$SelectAll">
    <keyboard-shortcut first-keystroke="ctrl a" second-keystroke="ctrl a" />
  </action>
  <action id="ActivateProjectToolWindow">
    <keyboard-shortcut first-keystroke="alt 1" />
    <keyboard-shortcut first-keystroke="alt ampersand" />
  </action>
  <action id="ActivateStructureToolWindow">
    <keyboard-shortcut first-keystroke="alt 7" />
    <keyboard-shortcut first-keystroke="alt #10000e9" />
  </action>
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="ctrl a" second-keystroke="ctrl d" />
  </action>
  <action id="CommentByLineComment">
    <keyboard-shortcut first-keystroke="ctrl slash" />
    <keyboard-shortcut first-keystroke="ctrl divide" />
    <keyboard-shortcut first-keystroke="ctrl colon" />
  </action>
  <action id="Editor SelectAll">
    <keyboard-shortcut first-keystroke="ctrl a" second-keystroke="ctrl a" />
  </action>
  <action id="EditorMoveToPageBottom" />
  <action id="EditorMoveToPageTop" />
